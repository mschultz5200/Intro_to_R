Name <- c("Jeb", 'Donald', "Ted", "Marco", "Carly", "Hilary", "Bernie")

ABC_political_poll_results <- c(4, 62, 51, 21, 2, 14, 15)
CBS_political_poll_results <- c(12, 75, 43, 19, 1, 21, 19) 

df <- data.frame(ABC_political_poll_results, CBS_political_poll_results)

colnames(df) <- c("ABC", "NBC")
rownames(df) <- Name

df
mean(df$ABC)
mean(df$NBC)
